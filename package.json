{
  "name": "@digitalbazaar/vc-status-list",
  "version": "2.1.1-0",
  "description": "Verifiable Credentials Status List",
  "license": "BSD-3-Clause",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-node",
    "test-node": "cross-env NODE_ENV=test mocha -r esm --preserve-symlinks -t 30000 -A -R ${REPORTER:-spec} --require tests/test-mocha.js tests/*.spec.js",
    "test-karma": "karma start karma.conf.js",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary npm run test-node",
    "coverage-ci": "cross-env NODE_ENV=test nyc --reporter=lcovonly npm run test-node",
    "coverage-report": "nyc report",
    "lint": "eslint ."
  },
  "files": [
    "StatusList.js",
    "index.js",
    "main.js"
  ],
  "dependencies": {
    "@digitalbazaar/bitstring": "^1.1.0",
    "@digitalbazaar/vc": "^2.1.0",
    "credentials-context": "^2.0.0",
    "@digitalbazaar/vc-status-list-context": "^3.0.0",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@digitalbazaar/did-method-key": "^2.0.0",
    "@digitalbazaar/ed25519-signature-2020": "^3.0.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^7.26.0",
    "eslint-config-digitalbazaar": "^2.8.0",
    "jsonld-signatures": "^9.0.2",
    "karma": "^6.3.2",
    "karma-babel-preprocessor": "^8.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^5.0.0",
    "mocha": "^8.4.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.0.0",
    "webpack": "^5.37.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalbazaar/vc-status-list"
  },
  "keywords": [
    "vc",
    "verifiable credential",
    "status list",
    "bitstring",
    "StatusList2021",
    "VerifiableCredential"
  ],
  "author": {
    "name": "Digital Bazaar, Inc.",
    "email": "support@digitalbazaar.com",
    "url": "https://digitalbazaar.com/"
  },
  "bugs": {
    "url": "https://github.com/digitalbazaar/vc-status-list/issues"
  },
  "homepage": "https://github.com/digitalbazaar/vc-status-list",
  "module": "main.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  }
}
